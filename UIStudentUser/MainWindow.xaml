<Window x:Class="UIStudentUser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UIStudentUser"
        xmlns:local2="clr-namespace:UIStudentUser.Component.Student"
        xmlns:local3="clr-namespace:UIStudentUser.Component.User"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label>Students</Label>
        <local2:StudentList x:Name="studentsList" Padding="2" />
        <Label>Users</Label>
        <local3:UserList x:Name="userList" Padding="2" />
    </Grid>
</Window>
